{
  "containerDefinitions": [
    {
      "name": "spring-template",
      "image": "390029653531.dkr.ecr.eu-central-1.amazonaws.com/spring-template:latest",
      "links": [],
      "portMappings": [
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": [],
      "command": [],
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
